* Measure JNWG00_CORE
.control

load {cicname}.raw

echo "MEAS_START"

foreach vtemp {temp_sweep}

set fend = .raw
load {cicname}_$vtemp$fend

meas tran ibp_$vtemp find i(vibp) when v(valid)=0.5
meas tran fine_$vtemp find v(d_fine) when v(valid)=0.5
meas tran coarse_$vtemp find v(d_coarse) when v(valid)=0.5

end


echo "MEAS_END"
.endc
